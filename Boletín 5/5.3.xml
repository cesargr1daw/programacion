<arboles>
    <especie1>
        <especie>Acer monspessulanum</especie>
            <nombre_comun>Arce de Montpeiller, Arce menor</nombre_comun>
            <vegetación>Caducfolio</vegetación>
            <altura>De 6 a 10 metros</altura>
            <forma_estructura>Copa esférica. Tronco principal recto con bifurcaciones. Ramaje colgante</forma_estructura>
            <color_primavera>Haz verde brillante, envés verde blanquecina</color_primavera>
            <resistencia>Heladas fuertes (hasta -15ºC)</resistencia><!-- Se refiere a la resistencia de las heladas-->
    </especie1>
    <especie2>
        <especie>Olea europea</especie>
            <nombre_comun>Olivo</nombre_comun>
            <vegetación>Perenne</vegetación>
            <altura>De 8 a 15 metros</altura>
            <forma_estructura>Copa Irregular. Tronco principal irregular con bifurcaciones</forma_estructura>
    </especie2>
    <especie3>
        <especie>Remaje tortuoso</especie>
            <color_primavera>Haz verde oscuro, envés verde plateado</color_primavera>
            <resistencia>Heladas fuertes (hasta -10ºC)</resistencia><!-- Se refiere a la resistencia de las heladas-->
    </especie3>
    <especie4>
        <especie>Platanus orientalis</especie>
            <nombre_comun>Plátano</nombre_comun>
            <vegetación>Caducifolio</vegetación>
            <altura>De 20 a 25 metros</altura>
            <forma_estructura></forma_estructura>
            <color_primavera>Haz verde medio, enves verde claro</color_primavera>
            <color_otoño>Ocre</color_otoño>
            <resistencia>Heladas fuertes (hasta -20ºC)</resistencia><!-- Se refiere a la resistencia de las heladas-->
    </especie4>
