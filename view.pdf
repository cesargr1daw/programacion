        changeBodyClass = function (clasName, action) { 
            if (action == 'add') {
                $('body').addClass(clasName);
            } else {
                $('body').removeClass(clasName);
                $('.app-cookies').remove();
            }
        }
    
        tabFocusRestrictor = function (lastItem, firstItem) { 
            $(lastItem).on('blur', function () {
                $(firstItem).trigger('focus');
            });
        }
    
        checkCookiesConsent = function () { 
            var getCookiesConsent = localStorage.getItem("cookiesConsent");
            
            if (getCookiesConsent) {
                changeBodyClass('cookies-consent', 'remove')
            } else {
                changeBodyClass('cookies-consent', 'add')
                $('#emailConfm').trigger('focus', '#todas_cookies');
            }
        }
    
        if (!$('body').hasClass('cookies-consent')) {
